<% if @users.present? %>
<table class="hover">
    <thead>
      <tr>
        <td>Email</td>
        <td>Role</td>
        <td>Update</td>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
        <td>
          <%= link_to 'See info' ,user_path(user)%>
          |
          <%= link_to 'Edit User',edit_user_path(user)%>
          |
          <%= link_to 'Delet User',user_path(user),
         method: :delete, class: "delete-item" %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
<%end %>
<%= link_to 'New User', new_user_path(@user)%>
